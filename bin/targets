#!/bin/bash
source lib/targets

main() {
  # revisar las redes con mk
  revisar_redes >&2

  for f in `datos`; do
    # s√≥lo agregar a la cola si pasaron las pruebas
    test -e `echo "$f" | bin/input2check` || continue

    # combinar las redes con cada uno de sus genes
    adj_from_txt "$f"
  done
}

main "$@"
